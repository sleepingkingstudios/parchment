{
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "overrides": [
    {
      "files": ["*.test.js", "*.test.jsx"],
      "env": {
        "jest": true
      }
    }
  ],
  "rules": {
    "react/jsx-one-expression-per-line": "off",
    "react/prop-types": ["error", { "skipUndeclared": true }]
  }
}
